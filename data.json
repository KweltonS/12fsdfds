    // –û—á–∏—Å—Ç–∫–∞ –ø–ª–µ–π–ª–∏—Å—Ç–∞\n        function clearPlaylist() {\n            if (confirm('–û—á–∏—Å—Ç–∏—Ç—å –≤–µ—Å—å –ø–ª–µ–π–ª–∏—Å—Ç?')) {\n                // –û—Å–≤–æ–±–æ–∂–¥–∞–µ–º URL –æ–±—ä–µ–∫—Ç–æ–≤\n                playlist.forEach(track => {\n                    if (track.url) {\n                        URL.revokeObjectURL(track.url);\n                    }\n                });\n                \n                playlist = [];\n                currentTrackIndex = -1;\n                audioPlayer.src = '';\n                document.getElementById('currentTrack').textContent = '–í—ã–±–µ—Ä–∏—Ç–µ —Ç—Ä–µ–∫ –¥–ª—è –≤–æ—Å–ø—Ä–æ–∏–∑–≤–µ–¥–µ–Ω–∏—è';\n                renderPlaylist();\n            }\n        }\n\n        // –û—Ç—Ä–∏—Å–æ–≤–∫–∞ –ø–ª–µ–π–ª–∏—Å—Ç–∞\n        function renderPlaylist() {\n            const playlistElement = document.getElementById('playlist');\n            \n            if (playlist.length === 0) {\n                playlistElement.innerHTML = '<div style=\"padding: 20px; text-align: center; color: #666;\">–ü–ª–µ–π–ª–∏—Å—Ç –ø—É—Å—Ç. –î–æ–±–∞–≤—å—Ç–µ –º—É–∑—ã–∫—É!</div>';\n                return;\n            }\n            \n            playlistElement.innerHTML = playlist.map((track, index) => `\n                <div class=\"track-item ${index === currentTrackIndex ? 'playing' : ''}\" \n                     onclick=\"playTrack(${index})\">\n                    üéµ ${track.name}\n                    <button onclick=\"event.stopPropagation(); removeTrack(${index})\" \n                            style=\"float: right; background: #e74c3c; color: white; border: none; padding: 2px 8px; border-radius: 3px; cursor: pointer;\">\n                        ‚úï\n                    </button>\n                </div>\n            `).join('');\n        }\n\n        // –£–¥–∞–ª–µ–Ω–∏–µ —Ç—Ä–µ–∫–∞\n        function removeTrack(index) {\n            // –û—Å–≤–æ–±–æ–∂–¥–∞–µ–º URL\n            if (playlist[index].url) {\n                URL.revokeObjectURL(playlist[index].url);\n            }\n            \n            playlist.splice(index, 1);\n            \n            if (currentTrackIndex === index) {\n                currentTrackIndex = -1;\n                audioPlayer.src = '';\n                document.getElementById('currentTrack').textContent = '–í—ã–±–µ—Ä–∏—Ç–µ —Ç—Ä–µ–∫ –¥–ª—è –≤–æ—Å–ø—Ä–æ–∏–∑–≤–µ–¥–µ–Ω–∏—è';\n            } else if (currentTrackIndex > index) {\n                currentTrackIndex--;\n            }\n            \n            renderPlaylist();\n        }\n\n        // –ì–æ—Ä—è—á–∏–µ –∫–ª–∞–≤–∏—à–∏\n        document.addEventListener('keydown', function(e) {\n            if (e.code === 'Space') {\n                e.preventDefault();\n                playPause();\n            }\n        });\n\n        console.log('–ê—É–¥–∏–æ–ø–ª–µ–µ—Ä –∑–∞–≥—Ä—É–∂–µ–Ω! –ü—Ä–æ—Å—Ç–æ –Ω–∞–∂–º–∏—Ç–µ \"–î–æ–±–∞–≤–∏—Ç—å –º—É–∑—ã–∫—É\"');\n    </script>\n</body>\n</html>",true,"","fullscreen; autoplay; encrypted-media",false,"allow-scripts"],""]],[],0,true,false,false,[]]],[],[]]],[["–°–ø–∏—Å–æ–∫ —Å–æ–±—ã—Ç–∏–π 1",[]]],[],"media/",false,1920,1080,4,false,"trilinear",false,"1.0.0.0",false,false,4,2,3,false,true,1,true,0.7853981633974483,[],"icons/",[],"normalized","3quh8uv67wo","fonts/",[["–®–∫–∞–ª–∞ 1",5,0.1,"default","default",[],0,0,1,"",1,1]],"high-performance",[],1760902432035,"vsync","com.Music.test","icons/–ò–∫–æ–Ω–∫–∞-–∑–∞–≥—Ä—É–∑–∫–∏.png",false,4,false,null,[],"folders",1,10000,false,[["–ë–ª–æ–∫-—Å—Ö–µ–º–∞ 1",[]]],"auto"]}